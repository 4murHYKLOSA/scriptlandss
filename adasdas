local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()

local GUI = Mercury:Create{
    Name = "Scriptland",
    Size = UDim2.fromOffset(600, 400),
    Theme = Mercury.Themes.Dark,
    Link = "https://github.com/deeeity/mercury-lib"
}

local Visual = GUI:Tab{
	Name = "visual",
	Icon = "rbxassetid://14317644795"
}

Visual:Button{
	Name = "player esp..!",
	Description = "h-here! let me highlight them for you...",
	Callback = function()
    -- Properties for the highlight
while wait() do
local highlightSettings = {
    DepthMode = Enum.HighlightDepthMode.AlwaysOnTop,
    FillColor = Color3.fromRGB(133, 227, 255),
    FillTransparency = 0.8,
    OutlineColor = Color3.fromRGB(231, 255, 251),
    OutlineTransparency = 0.6
}

-- Function to add highlight to a player
local function addHighlightToPlayer(player)
    local character = player.Character or player.CharacterAdded:Wait()
    if not character:FindFirstChild("Highlight") then
        local highlight = Instance.new("Highlight")
        highlight.DepthMode = highlightSettings.DepthMode
        highlight.FillColor = highlightSettings.FillColor
        highlight.FillTransparency = highlightSettings.FillTransparency
        highlight.OutlineColor = highlightSettings.OutlineColor
        highlight.OutlineTransparency = highlightSettings.OutlineTransparency
        highlight.Name = "Highlight"
        highlight.Parent = character
    end
end

-- Apply highlight to all players
for _, player in ipairs(game.Players:GetPlayers()) do
    addHighlightToPlayer(player)
end

-- Add highlight to players who join later
game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        addHighlightToPlayer(player)
    end)
end)
end
     end
}

local LocalPlayer = GUI:Tab{
	Name = "player",
	Icon = "rbxassetid://133311386579125"
}

LocalPlayer:Slider{
	Name = "walkin' sped!",
	Default = 16, -- Default WalkSpeed in Roblox
	Min = 16, -- Minimum WalkSpeed
	Max = 100, -- Maximum WalkSpeed
	Callback = function(value)
		local player = game.Players.LocalPlayer
		if player and player.Character and player.Character:FindFirstChild("Humanoid") then
			player.Character.Humanoid.WalkSpeed = value
		end
	end
}

LocalPlayer:Slider{
	Name = "jumping powa!",
	Default = 50, -- Default JumpPower in Roblox
	Min = 50, -- Minimum JumpPower
	Max = 100, -- Maximum JumpPower
	Callback = function(value)
		local player = game.Players.LocalPlayer
		if player and player.Character and player.Character:FindFirstChild("Humanoid") then
			player.Character.Humanoid.JumpPower = value
		end
	end
}

LocalPlayer:Button{
	Name = "max zoom distance...",
	Description = "makes the zoom distance higher so you can zoom far...!!",
	Callback = function()
    while wait() do
		local player = game.Players.LocalPlayer
		local camera = game.Workspace.CurrentCamera
		
		if player and camera then
			player.CameraMaxZoomDistance = 200 -- Set the MaxZoom distance
		end
	end
    end
}

LocalPlayer:Button{
    Name = "infinite s-stamina!! uwu...",
    Description = "dihs are so cute!",
    Callback = function()
        -- Destroy the specified scripts
        local starterPlayer = game:GetService("StarterPlayer")
        local workspace = game:GetService("Workspace")

        if starterPlayer.StarterCharacterScripts:FindFirstChild("Sprint") then
            starterPlayer.StarterCharacterScripts.Sprint:Destroy()
        end

        if workspace:FindFirstChild("Thecutecat_44") and workspace.Thecutecat_44:FindFirstChild("Sprint") then
            workspace.NEWnew010942.Sprint:Destroy()
        end

        -- Speed control script
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")

        local normalSpeed = 16
        local sprintSpeed = 22

        humanoid.WalkSpeed = normalSpeed

        -- User input setup
        local userInput = game:GetService("UserInputService")

        -- Create sprint toggle button
        local screenGui = Instance.new("ScreenGui", player.PlayerGui)
        local sprintButton = Instance.new("TextButton", screenGui)
        sprintButton.Size = UDim2.new(0.15, 0, 0.08, 0)
        sprintButton.Position = UDim2.new(0.75, 0, 0.85, 0)
        sprintButton.Text = "Toggle Sprint"
        sprintButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        sprintButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        sprintButton.Font = Enum.Font.SourceSansBold
        sprintButton.TextSize = 24

        -- Add rounded corners
        local uiCorner = Instance.new("UICorner", sprintButton)
        uiCorner.CornerRadius = UDim.new(0, 10)

        local isSprinting = false

        local function toggleSprint()
            isSprinting = not isSprinting
            humanoid.WalkSpeed = isSprinting and sprintSpeed or normalSpeed
            sprintButton.Text = isSprinting and "Sprinting" or "Walking"
            sprintButton.BackgroundColor3 = isSprinting and Color3.fromRGB(200, 50, 50) or Color3.fromRGB(50, 200, 50)
        end

        sprintButton.MouseButton1Click:Connect(toggleSprint)

        print("Sprint toggle button with adaptive size and position added and configured for mobile and PC.")
    end
}



local Blatant = GUI:Tab{
	Name = "blatant",
	Icon = "rbxassetid://10784674835"
}

Blatant:Button{
	Name = "clik teleport..!!",
	Description = "hold c-ctrl and click to teleport..",
	Callback = function()
		local player = game.Players.LocalPlayer
		local mouse = player:GetMouse()
		local userInputService = game:GetService("UserInputService")
		local ctrlHeld = false

		-- Detect when Ctrl is pressed or released
		userInputService.InputBegan:Connect(function(input, gameProcessed)
			if input.KeyCode == Enum.KeyCode.LeftControl then
				ctrlHeld = true
			end
		end)

		userInputService.InputEnded:Connect(function(input, gameProcessed)
			if input.KeyCode == Enum.KeyCode.LeftControl then
				ctrlHeld = false
			end
		end)

		-- Teleport only if Ctrl is held and mouse is clicked
		mouse.Button1Down:Connect(function()
			if ctrlHeld and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local humanoidRootPart = player.Character.HumanoidRootPart
				local targetPosition = mouse.Hit.p -- Get the clicked position
				humanoidRootPart.CFrame = CFrame.new(targetPosition) -- Teleport the player
			end
		end)
	end
}

Blatant:Button{
    Name = "fix lag... [lightning]", -- Name of the button
    Description = "click to reduce lag by making lightning ugly...", -- Optional description
    Callback = function()
        -- Get the Lighting service
        local lighting = game:GetService("Lighting")

        -- Set the lighting technology to Compatibility
        lighting.Technology = Enum.Technology.Compatibility

        print("l-l-lighting technology has been set to Compatibility!")
    end
}

local BoilingDeath = GUI:Tab{
	Name = "boiling death",
	Icon = "rbxassetid://17642922090"
}

BoilingDeath:Button{
    Name = "remove camera shake!!! :4", -- Name of the button
    Description = "c-click to remove camera shake...", -- Optional description
    Callback = function()
        -- Path to the CamShake object
        local camShake = workspace:FindFirstChild("Thecutecat_44"):FindFirstChild("CamShake")

        -- Remove CamShake if it exists
        if camShake then
            camShake:Destroy()
            print("camera shake has been removed from workspace.LocalPlayer.")
        else
            print("camera shake does not exist in workspace.LocalPlayer?!!")
        end
    end
}

local DeadOrAlive = GUI:Tab{
	Name = "dead or skibidi",
	Icon = "rbxassetid://11967385182"
}

DeadOrAlive:Button{
    Name = "destroy all doors..!! (not sure if working)",
    Description = "you like cocuk por",
    Callback = function()
        for i = 1, 10 do
            local door = workspace.DeadOrAlive.GoodRooms[i].Doors
            if door then
                door:Destroy()
            end
        end
    end
}
